package functions

import (
	//l "github.com/JoergReinhardt/godeep/lang"
	d "github.com/JoergReinhardt/godeep/data"
)

func prec(f ...d.Type) tokens {
	var t = make(tokens, 0, len(f))

}
func putAppend(last token, tok tokens) tokens {
	return append(tok, last)
}
func putFront(first token, tok tokens) tokens {
	return append(tokens{first}, tok...)
}
func insert(ins token, tok tokens) tokens {
	var args = tokens{}
	for i, t := range tok {
		args = append(args, t)
		if i < len(tok)-1 {
			args = append(args, ins)
		}
	}
	return args
}
func enclose(left, right token, tok tokens) tokens {
	var args = tokens{left}
	for _, t := range tok {
		args = append(args, t)
	}
	args = append(args, right)
	return args
}
